{
    "manifest_version": 3,
    "name": "Collate Tabs",
    "description": "Reorder tabs so tabs of the same domain are together.",
    "version": "0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": []
}
